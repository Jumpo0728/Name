{
  "format_version": "1.1.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:player",
      "textures": {
        "default": "textures/entity/player",
        "slim": "textures/entity/player_slim"
      },
      "geometry": {
        "default": "geometry.humanoid"
      },
      "animations": {
        "walking": "animation.player.walking",
        "flying": "animation.player.flying",
        "attack": "animation.player.attack",
        "idle": "animation.player.idle"
      },
      "scripts": {
        "animate": [
          "controller.animation.player"
        ]
      },
      "render_controllers": [
        "controller.render.player"
      ]
    }
  }
}
